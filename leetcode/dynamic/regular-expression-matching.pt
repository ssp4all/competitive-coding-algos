https://leetcode.com/problems/regular-expression-matching

class Solution:
    def isMatch(self, t: str, p: str) -> bool:
        if not p:
            return not t
        fm = bool(t) and p[0] in {'.', t[0]}
        
        if len(p) >= 2 and p[1] == "*":
            return self.isMatch(t, p[2:]) or \
                    (fm and self.isMatch(t[1:], p))
        else:
            return fm and self.isMatch(t[1:], p[1:])